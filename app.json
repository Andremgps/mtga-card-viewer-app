{
  "expo": {
    "name": "MTGA Card Viewer",
    "slug": "mtga-card-viewer-app",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "mtga.card.viewer",
      "enableDangerousExperimentalLeanBuilds": true,
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-5464447246309753~4292933841"
      }
    }
  }
}